    "Topology",
    "Geometry"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2015-rollup": "1.2.0",
    "babel-register": "6.18.0",
    "coveralls": "2.11.14",
    "expect.js": "0.3.1",
    "git-rev-sync": "1.8.0",
    "jsdoc": "jsdoc3/jsdoc",
    "jsts": "1.2.1",
    "mocha": "3.1.2",
    "mocha-lcov-reporter": "1.2.0",
    "nyc": "8.3.2",
    "rbush": "2.0.1",
    "rollup": "0.36.3",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "1.1.1",
    "standard": "8.5.0",
    "uglify-js": "2.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bjornharrtell/topolis.git"
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "beforeEach"
    ]
  },
  "scripts": {
