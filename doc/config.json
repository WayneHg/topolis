{
    "opts": {
        "recurse": true,
        "destination": "dist/doc"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": [
        ],
        "include": [
          "src"
        ]
    },
    "plugins": [
        "node_modules/jsdoc/plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "systemName": "topolis",
        "copyright": "Bj√∂rn Harrtell",
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles": true,
            "useLongnameInNav": false
        },
        "applicationName": "topolis"
    }
}
